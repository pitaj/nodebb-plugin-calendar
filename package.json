{
  "name": "nodebb-plugin-calendar",
  "version": "1.0.0-alpha.39",
  "description": "Calendar and events for NodeBB",
  "main": "library.js",
  "scripts": {
    "test": "npm run lint && babel-node ./src/lib/tests",
    "babel": "babel src --out-dir build --source-maps",
    "build": "set NODE_ENV=development&& npm run babel && webpack --progress && node ../nodebb/nodebb build",
    "build-production": "npm run babel && set NODE_ENV=production && webpack --progress",
    "release": "npm run test && npm run build-production && npm version prerelease && npm publish",
    "lint": "eslint ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pitaj/nodebb-plugin-calendar.git"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "calendar",
    "event",
    "date"
  ],
  "author": "PitaJ",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pitaj/nodebb-plugin-calendar/issues"
  },
  "homepage": "https://github.com/pitaj/nodebb-plugin-calendar#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-module-method": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-function-name": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node4": "^2.1.1",
    "bundle-loader": "^0.5.5",
    "chalk": "^1.1.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.3.0",
    "uglify-js": "^2.8.26",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "core-js": "^2.4.1",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "fullcalendar": "^3.4.0",
    "moment": "^2.18.1",
    "source-map-support": "^0.4.15",
    "validator": "^7.0.0"
  },
  "nbbpm": {
    "compatibility": "^1.5.1"
  }
}
